node("workstation"){
  try {
    withCredentials([usernameColonPassword(credentialsId: 'ssh', variable: 'USERPASS')]) {
      stage("one") {
         echo "one"
         sh "env"
      }
    }

      stage("two") {
           echo "two"
      }
    }catch (e){
      stage("Declarative post actions") {
        echo "This will run when itz a failure"
    }
  }
}