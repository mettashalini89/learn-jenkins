pipeline {
  agent { node { label 'terraform' } }

  environment {
    SAMPLE_URL = "google.com"
    SSH = credentials("SSH")
  }

  stages{
    stage("one"){
      steps {
        echo "one"
      }
    }

    stage("two") {
      steps {
        echo "two"
      }
    }
  }
  post {
    failure {
      echo 'I will always say Hello again!'
    }
  }
}